<article class="site-page" data-template-page>
  {% comment %}

  <header class="page-masthead">
    <h1 class="page-title">
      {{ page.title }}
    </h1>
  </header> 

  {% if page.content != blank %}
  <div class="page-content rte">
    {{ page.content }}
  </div>
  {% endif %}

  {% endcomment %}

  {% section 'about_slider'%}
  
  {% section 'about_story'%}
  
  {% section 'about'%}

</article>
